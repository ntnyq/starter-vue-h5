  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.2.1",
    "js-cookie": "^2.2.1",
    "qs": "^6.8.0",
    "vant": "^2.1.7",
    "vue": "^2.6.10",
    "vue-router": "^3.1.2",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.12.1",
    "eslint": "^6.2.2",
    "eslint-config-goy": "^0.0.4",
    "eslint-plugin-vue": "^5.2.3",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "lint-staged": "^9.2.5",
    "postcss-pxtorem": "^4.0.1",
    "sass": "^1.22.10",
