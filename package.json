    "script-ext-html-webpack-plugin": "^2.1.4",
    "svg-sprite-loader": "^4.1.6",
    "svgo": "^1.3.0",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ntnyq/vue-vant-template.git"
